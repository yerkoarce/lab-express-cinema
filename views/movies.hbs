<h1 class="title-movies">Click on the movie to check the showtimes!</h1>

<div class="search-container">
  {{#each movies}}
    <div class="movie-container">
      <img src="{{ image }}" alt="{{ title }}">
      <h3>{{ title }}</h3>
      <a href="/movies/{{_id}}">See more</a>
    </div>
  {{/each}}
</div>
